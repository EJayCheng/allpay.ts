{
  "name": "opay-ts",
  "version": "0.0.10",
  "description": "oPay API. typescript version.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test":
      "rm -rf dist && tsc && NODE_ENV=test && NODE_ENV=test && ./node_modules/.bin/mocha ./dist/test/*.spec.js",
    "prepublish": "tsc",
    "coverage":
      "rm -rf coverage && tsc && NODE_ENV=test && ./node_modules/.bin/nyc npm run test"
  },
  "nyc": {
    "include": ["dist/**/*.js"],
    "exclude": ["**/*.spec.js"],
    "extension": [".js"],
    "require": ["ts-node/register"],
    "reporter": ["text-summary", "html"],
    "all": true
  },
  "repository": "https://github.com/EJayCheng/oPay-ts.git",
  "author": "EJay <gto830226@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.102",
    "@types/node": "^9.4.4",
    "@types/request-promise": "^4.1.41",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/mocha": "^2.2.48",
    "@types/should": "^13.0.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "should": "^13.2.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  }
}
